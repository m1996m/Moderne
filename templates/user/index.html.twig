{% extends 'base.html.twig' %}

{% block title %}index user{% endblock %}

{% block body %}

<div class="container">
    <div class="row">
        <div class="col-md-4">
          <div class="card">
            <div class="row">
                <div class="col-md-6">
                    <img src="" alt="">
                      {% for user in users %}
                      <tr>
                          <td>{{ user.nom }}</td>
                      </tr>
                  {% else %}
                      <tr>
                          <td colspan="3">no records found</td>
                      </tr>
                  {% endfor %}
                </div>
            </div>
          </div>
        </div>
    </div>
</div>
{% endblock %}
